package ClassesThatNest;

import ClassesThatNest.Computer;

public class NonStaticMembers {
	public static void main(String[] args) {
		Computer c = new Computer();
		
		c.hauptspeicher.printHersteller();
		c.hauptspeicher.printHsNumber();
		c.hauptspeicher.zelle.printNumberOfSpeicherzellen();
		
		Computer.Hauptspeicher h = c.new Hauptspeicher() ; //Creation of additional Instances is also possible
		h.printHersteller();
		Computer.Hauptspeicher.Speicherzelle z = h.new Speicherzelle();
		z.printNumberOfSpeicherzellen();
		Computer.Hauptspeicher h2 = c.new Hauptspeicher();
		h2.printHersteller();
		
		h.printHsNumber();
		h2.printHsNumber();
		
		Computer.Hauptspeicher.Speicherzelle zelle2 = new Computer().new Hauptspeicher().new Speicherzelle();
		zelle2.printNumberOfSpeicherzellen();
	}

}
